{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Swagger for Cocktail API",
    "description": "Open API docs with Swagger",
    "license": { "name": "MIT", "url": "http://localhost:5000" }
  },
  "host": "localhost:5000",
  "basePath": "/",
  "tags": [
    { "name": "User", "description": "User 로그인, 회원가입 API" },
    { "name": "Cocktail", "description": "칵테일 조회 API" },
    { "name": "User Cocktail", "description": "유저 칵테일 CRUD API" }
  ],
  "schemes": ["HTTP"],
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "paths": {
    "/": {
      "get": {
        "tags": ["Main"],
        "summary": "Main Page Get",
        "responses": { "200": { "description": "OK" } }
      }
    },
    "/cocktails/{name}": {
      "get": {
        "tags": ["Cocktail"],
        "summary": "특정 칵테일 조회",
        "parameters": [{ "in": "path", "name": "name" }],
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    },
    "/cocktails": {
      "get": {
        "tags": ["Cocktail"],
        "summary": "칵테일 전체 조회",
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    },
    "/cocktail": {
      "post": {
        "tags": ["Cocktail"],
        "summary": "칵테일 정보 입력",
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "ingredient": "body",
            "imageUrl": "body",
            "taste": "body",
            "description": "body",
            "userId": "body"
          },
          {
            "name": "Authorization",
            "in": "header",
            "description": "JWT access token",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    },
    "/cocktail/{name}": {
      "delete": {
        "tags": ["Cocktail"],
        "summary": "칵테일 정보 삭제",
        "parameters": [
          { "in": "path", "name": "name" },
          {
            "name": "authorization",
            "in": "header",
            "description": "JWT access token",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    },
    "/register": {
      "post": {
        "tags": ["User"],
        "summary": "register user Post",
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "email": "body",
            "password": "body",
            "schema": { "$ref": "#/definitions/User" }
          }
        ],
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    },
    "/login": {
      "post": {
        "tags": ["User"],
        "summary": "login user Post",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "email": "body",
            "password": "body",
            "required": true,
            "schema": { "$ref": "#/definitions/User2" }
          }
        ],
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    },
    "/auth/google": {
      "get": {
        "tags": ["User"],
        "summary": "구글 login",
        "description": "해당 path로 로그인 후엔 /login로 돌아갑니다.",
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    },
    "/refresh": {
      "get": {
        "tags": ["User"],
        "summary": ["Access Token 재발급"],
        "description": "access token은 토큰 앞에 'Bearer ' 꼭 붙여서 request 해야합니다.",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "description": "JWT access token",
            "required": true,
            "type": "string"
          },
          {
            "name": "refresh",
            "in": "header",
            "description": "JWT refresh token",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    },
    "/addLike/{id}": {
      "post": {
        "tags": ["Like"],
        "summary": "좋아요 추가",
        "parameters": [
          { "in": "path", "name": "id" },
          {
            "name": "Authorization",
            "in": "header",
            "description": "JWT access token",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    },
    "/deleteLike/{id}": {
      "delete": {
        "tags": ["Like"],
        "summary": "좋아요 삭제",
        "parameters": [
          { "in": "path", "name": "id" },
          {
            "name": "Authorization",
            "in": "header",
            "description": "JWT access token",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    },
    "/likeliet": {
      "get": {
        "tags": ["Like"],
        "summary": "좋아요 전체 리스트",
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "description": "JWT access token",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    },
    "/cocktailLike/{id}": {
      "get": {
        "tags": ["Like"],
        "summary": "칵테일이 받은 좋아요 리스트",
        "parameters": [
          { "in": "path", "name": "id" },
          {
            "name": "Authorization",
            "in": "header",
            "description": "JWT access token",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    },
    "/userLike/{id}": {
      "get": {
        "tags": ["Like"],
        "summary": "유저가 좋아요한 리스트 목록",
        "parameters": [
          { "in": "path", "name": "id" },
          {
            "name": "Authorization",
            "in": "header",
            "description": "JWT access token",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    },
    "/rank10": {
      "get": {
        "tags": ["Rank"],
        "summary": "Rank 상위 10개 데이터 반환",
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    },
    "/rank/{name}": {
      "get": {
        "tags": ["Rank"],
        "summary": "Rank 내 특정 칵테일 정보 반환",
        "parameters": [{ "in": "path", "name": "name" }],
        "responses": {
          "201": { "description": "OK" },
          "404": { "description": "Failed. Bad Request" }
        }
      }
    }
  },
  "definitions": {
    "Cocktail": {
      "properties": {
        "name": { "type": "string" },
        "ingredient": { "type": "[string]" },
        "imageUrl": { "type": "string" },
        "rank": { "type": "Schema.Types.ObjectId" }
      }
    },
    "User": {
      "properties": {
        "email": { "type": "string" },
        "password": { "type": "string" },
        "name": { "type": "string" }
      }
    },
    "User2": {
      "properties": {
        "email": { "type": "string" },
        "password": { "type": "string" }
      }
    },
    "Like": {
      "properties": {
        "giveUserId": { "type": "string" },
        "getCocktailId": { "type": "string" },
        "name": { "type": "string" }
      }
    },
    "Rank": {
      "properties": {
        "bane": { "type": "string" },
        "visitors": { "type": "string" },
        "years": { "type": "string" },
        "rank": { "type": "string" }
      }
    }
  }
}
